{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ServiceAdvisorReadOnlyPermissions",
            "Effect": "Allow",
            "Action": [
                "ec2:Describe*",
                "rds:Describe*",
                "rds:List*",
